{
  "private": true,
  "name": "reactabular-monorepo",
  "version": "1.0.0",
  "description": "Spectacular tables for React",
  "scripts": {
    "init": "npm install && lerna bootstrap",
    "start": "webpack-dev-server",
    "test": "karma start --",
    "test:tdd": "karma start --auto-watch --no-single-run --",
    "test:lint": "eslint . --ext .js --ext .jsx --cache",
    "gh-pages:build-and-deploy": "npm run gh-pages:build && npm run gh-pages:deploy",
    "gh-pages:build": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --json --profile > stats.json",
    "dist:update": "npm run dist:all && git commit --allow-empty -am \"Update dist\"",
    "dist:all": "npm run dist:build && npm run dist:build-min",
    "dist:build": "webpack",
    "dist:build-min": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactabular/reactabular.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "tables"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactabular/reactabular/issues"
  },
  "homepage": "http://reactabular.js.org/",
  "dependencies": {},
  "devDependencies": {
    "annogenerate": "^0.8.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.11.6",
    "catalog": "^2.4.6",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "git-prepush-hook": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "lerna": "2.0.0-beta.25",
    "lodash": "^4.15.0",
    "mocha": "^3.0.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "purecss": "^0.6.0",
    "raw-loader": "^0.5.1",
